{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": false,
    "skipLibCheck": true,
    "typeRoots": ["../../node_modules/@types"],
    "resolveJsonModule": true
  },
  "include": ["**/*.ts", "../../deno.d.ts"]
}
